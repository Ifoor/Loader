ration=0.2 r.TintColor=Color3.fromRGB(255,240,220)r.Name="ColorCorrectionEffect"r.Parent=a local s=Instance.new("DepthOfFieldEffect")s.FocusDistance=20 s.InFocusRadius=50 s.NearIntensity=0.5 s.FarIntensity=0.5 s.Name="DepthOfFieldEffect"s.Parent=a end local function t()local u=c:Create(i,TweenInfo.new(1,Enum.EasingStyle.Quint,Enum.EasingDirection.Out),{Position=UDim2.new(0.5,-150,-0.5,0)})u:Play()wait(1)h:Destroy()end m.MouseButton1Click:Connect(function()if l.Text=="MaxLig"then p()t()else l.Text=""l.PlaceholderText="Неверный ключ!"l.BackgroundColor3=Color3.fromRGB(200,50,50)wait(1)l.BackgroundColor3=Color3.fromRGB(50,50,50)l.PlaceholderText="Введите ключ"end end)n.MouseButton1Click:Connect(function()setclipboard("https://discord.gg/aq87xSYpQ6")n.Text="Copied!"wait(1)n.Text="Discord"end)d.InputBegan:Connect(function(v,w)if not w and v.KeyCode==Enum.KeyCode.Return then m:MouseButton1Click()end end)

local W=loadstring(game:HttpGet("https://tree-hub.vercel.app/api/UI/WindUI"))():CreateWindow({Title="Key System",Icon="key",Author="YourName",Folder="YourHub",Size=UDim2.fromOffset(400,300),KeySystem={Key={"1234","5678"},Note="Enter the correct key to proceed.",SaveKey=true},Transparent=true,Theme="Dark"}):Tab({Title="Enter Key",Icon="key"}) local K,E={"AdminCh","Character2"},"" W:Input({Title="Enter Key",Default="",Placeholder="Enter your key here",Callback=function(i)E=i end}) W:Button({Title="Submit Key",Desc="Check the entered key",Callback=function() if table.find(K,E) then W:Notify({Title="Access Granted",Content="Correct Key! Loading script...",Duration=3}) wait(3.5) loadstring(game:HttpGet("https://raw.githubusercontent.com/Ifoor/Loader/refs/heads/main/script/Figure%20Battlegrounds",true))() else W:Notify({Title="Access Denied",Content="Incorrect Key! Try again.",Duration=3}) end end}) W:Button({Title="Copy Discord Link",Desc="Click to copy our Discord link",Callback=function() setclipboard("https://discord.gg/VgvJ7S25EZ") W:Notify({Title="Copied!",Content="Discord link copied to clipboard.",Duration=3}) end})

local w=loadstring(game:HttpGet("https://tree-hub.vercel.app/api/UI/WindUI"))():CreateWindow({Title="Roblox",Icon="gamepad",Author="YourName",Folder="CheatHub",Size=UDim2.fromOffset(600,500),Transparent=true,Theme="Dark",SideBarWidth=200})local m=w:Tab({Title="Main Cheats",Icon="zap"})local e=w:Tab({Title="ESP",Icon="eye"})local c=w:Tab({Title="Settings",Icon="keyboard"})m:Toggle({Title="God Mode",Default=false,Callback=function(s)local p=game.Players.LocalPlayer if s then p.Character.Humanoid.MaxHealth=math.huge p.Character.Humanoid.Health=math.huge else p.Character.Humanoid.MaxHealth=100 end end})local f=false m:Toggle({Title="Fly Mode",Default=false,Callback=function(s)local p=game.Players.LocalPlayer local r=p.Character and p.Character:FindFirstChild("HumanoidRootPart")if s and r then f=true local uis=game:GetService("UserInputService")local rs=game:GetService("RunService")local sp=50 local d=Vector3.new(0,0,0)local function b(i)if i.KeyCode==Enum.KeyCode.W then d=Vector3.new(0,0,-1)elseif i.KeyCode==Enum.KeyCode.S then d=Vector3.new(0,0,1)elseif i.KeyCode==Enum.KeyCode.A then d=Vector3.new(-1,0,0)elseif i.KeyCode==Enum.KeyCode.D then d=Vector3.new(1,0,0)elseif i.KeyCode==Enum.KeyCode.Space then d=Vector3.new(0,1,0)elseif i.KeyCode==Enum.KeyCode.LeftShift then d=Vector3.new(0,-1,0)end end local function e(i)if i.KeyCode==Enum.KeyCode.W or i.KeyCode==Enum.KeyCode.S or i.KeyCode==Enum.KeyCode.A or i.KeyCode==Enum.KeyCode.D or i.KeyCode==Enum.KeyCode.Space or i.KeyCode==Enum.KeyCode.LeftShift then d=Vector3.new(0,0,0)end end local fl rs.RenderStepped:Connect(function()if f and r then local cam=workspace.CurrentCamera local md=cam.CFrame:VectorToWorldSpace(d)r.Velocity=md*sp else fl:Disconnect()end end)uis.InputBegan:Connect(b)uis.InputEnded:Connect(e)else f=false r.Velocity=Vector3.new(0,0,0)end end})local u=game:GetService("UserInputService")local pl=game.Players.LocalPlayer local j=true u.JumpRequest:Connect(function()if j then pl.Character:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)end end)e:Toggle({Title="ESP Players",Default=false,Callback=function(s)for _,p in pairs(game.Players:GetPlayers())do if p~=game.Players.LocalPlayer then local h=Instance.new("Highlight",p.Character)h.FillColor=Color3.new(1,0,0)h.OutlineTransparency=0.5 h.Enabled=s end end end})local n=false m:Toggle({Title="NoClip",Default=false,Callback=function(s)n=s local ch=game.Players.LocalPlayer.Character while n do for _,pa in pairs(ch:GetChildren())do if pa:IsA("BasePart") then pa.CanCollide=false end end wait(0.1)end for _,pa in pairs(ch:GetChildren())do if pa:IsA("BasePart") then pa.CanCollide=true end end end})m:Button({Title="Give Teleport Tool",Callback=function()local t=Instance.new("Tool")t.Name="Teleport"t.RequiresHandle=false t.Parent=game.Players.LocalPlayer.Backpack t.Activated:Connect(function()local p=game.Players.LocalPlayer local m=p:GetMouse()if m.Target then p.Character.HumanoidRootPart.CFrame=CFrame.new(m.Hit.p)end end)end})local kb={["Fly Mode"]=Enum.KeyCode.F,["No Clip"]=Enum.KeyCode.N}c:Dropdown({Title="Select Cheat to Rebind Key",Values={"Fly Mode","No Clip"},Callback=function(s)local il il=game:GetService("UserInputService").InputBegan:Connect(function(i,pr)if not pr then kb[s]=i.KeyCode il:Disconnect()end end)end})game:GetService("UserInputService").InputBegan:Connect(function(i,pr)if not pr then for ch,k in pairs(kb)do if i.KeyCode==k then if ch=="Fly Mode"then f=not f elseif ch=="No Clip"then n=not n local ch=game.Players.LocalPlayer.Character while n do for _,pa in pairs(ch:GetChildren())do if pa:IsA("BasePart") then pa.CanCollide=false end end wait(0.1)end for _,pa in pairs(ch:GetChildren())do if pa:IsA("BasePart") then pa.CanCollide=true end end end end end end end)

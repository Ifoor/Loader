local WindUI, enteredKey, validKeys = loadstring(game:HttpGet("https://tree-hub.vercel.app/api/UI/WindUI"))(), "", { "AdminCh", "AdminCh" } local Window = WindUI:CreateWindow({ Title = "Key System", Icon = "key", Author = "YourName", Folder = "YourHub", Size = UDim2.fromOffset(400, 300), KeySystem = { Key = { "1234", "5678" }, Note = "Enter the correct key to proceed.", SaveKey = true }, Transparent = true, Theme = "Dark" }) local InputTab = Window:Tab({ Title = "Enter Key", Icon = "key" }) InputTab:Input({ Title = "Enter Key", Default = "", Placeholder = "Enter your key here", Callback = function(input) enteredKey = input end }) InputTab:Button({ Title = "Submit Key", Desc = "Check the entered key", Callback = function() WindUI:Notify({ Title = table.find(validKeys, enteredKey) and "Access Granted" or "Access Denied", Content = table.find(validKeys, enteredKey) and "Correct Key! Loading script..." or "Incorrect Key! Try again.", Duration = 3 }) if table.find(validKeys, enteredKey) then wait(3.5) loadstring(game:HttpGet("https://raw.githubusercontent.com/Ifoor/Loader/refs/heads/main/script/Figure%20Battlegrounds", true))() end end }) InputTab:Button({ Title = "Copy Discord Link", Desc = "Click to copy our Discord link", Callback = function() setclipboard("https://discord.gg/VgvJ7S25EZ") WindUI:Notify({ Title = "Copied!", Content = "Discord link copied to clipboard.", Duration = 3 }) end })
